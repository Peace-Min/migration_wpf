<Page
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Project_wpf.View"
      xmlns:ViewModel="clr-namespace:Project_wpf.ViewModel" x:Class="Project_wpf.View.AccountListView"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AccountListView"
       Background="{StaticResource backGround}"
 >
    <Page.DataContext>
        <ViewModel:AccountListViewModel/>
    </Page.DataContext>

    <Grid>
        <TextBox HorizontalAlignment="Left" Margin="125,0,0,0" TextWrapping="Wrap" Text="{Binding }" VerticalAlignment="Center" Width="120" Height="178"/>
        <TextBox HorizontalAlignment="Left" Margin="445,0,0,0" TextWrapping="Wrap" Text="{Binding Value}" VerticalAlignment="Center" Width="120" Height="178"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="322,331,0,0" VerticalAlignment="Top" Height="52" Width="78" Click="Button_Click_1"/>
    </Grid>
</Page>
